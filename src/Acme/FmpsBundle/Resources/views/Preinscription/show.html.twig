{% extends '::base.html.twig' %}

{% block body %}
<h3>Demande de préinscription</h3>

<table class="table table-hover">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nomenfant</th>
            <td>{{ entity.nomEnfant }}</td>
        </tr>
        <tr>
            <th>Prenomenfant</th>
            <td>{{ entity.prenomEnfant }}</td>
        </tr>
        <tr>
            <th>Nomtiteur</th>
            <td>{{ entity.nomTiteur }}</td>
        </tr>
        <tr>
            <th>Prenomtiteur</th>
            <td>{{ entity.prenomTiteur }}</td>
        </tr>
        <tr>
            <th>Sectionecoleid</th>
            <td>{{ entity.section }}</td>
        </tr>
        <tr>
            <th>Anneescolaireid</th>
            <td>{{ entity.anneeScolaire }}</td>
        </tr>
        <tr>
            <th>Ecoleid</th>
            <td>{{ entity.ecole }}</td>
        </tr>
        <tr>
            <th>Catégorie</th>
            <td>{{ entity.category }}</td>
        </tr>
        <tr>
            <th>Commentaire</th>
            <td>{{ entity.commentaire }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('preinscription') }}">
            <span class="back">Retour &agrave; la liste</span>
        </a>
    </li>
    <li>
        <a href="{{ path('preinscription_edit', { 'id': entity.id }) }}">
            <span class="edit">Editer</span>
        </a>
    </li>
    <li>
        <form action="{{ path('preinscription_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit" class="delete">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}