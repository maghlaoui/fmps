{% extends '::base.html.twig' %}

{% block body %}
<h3>Inscription dans une offre de service</h3>

<table class="table table-hover">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Offre de service</th>
            <td>{{ entity.offreService }}</td>
        </tr>
        <tr>
            <th>Enfant</th>
            <td>{{ entity.inscription }}</td>
        </tr>
        <tr>
            <th>Mois</th>
            <td>{{ entity.mois }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.statusStr }}</td>
        </tr>
				<tr>
            <th>Commentaire</th>
            <td>{{ entity.commentaire }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('inscriptionoffreservice') }}">
            <span class="back">Retour &agrave; la liste</span>
        </a>
    </li>
    <li>
        <a href="{{ path('inscriptionoffreservice_edit', { 'id': entity.id }) }}">
            <span class="edit">Editer</span>
        </a>
    </li>
    <li>
        <form action="{{ path('inscriptionoffreservice_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit" class="btn btn-danger" data-confirm="êtes vous sûre de vouloir supprimer?"><i class="icon-trash icon-white"></i> Supprimer</button>
        </form>
    </li>
</ul>
{% endblock %}