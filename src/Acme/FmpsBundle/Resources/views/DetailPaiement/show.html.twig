{% extends '::base.html.twig' %}

{% block body %}
<h3>DetailPaiement</h3>

<table class="table table-hover">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Moisid</th>
            <td>{{ entity.moisId }}</td>
        </tr>
        <tr>
            <th>Serviceid</th>
            <td>{{ entity.serviceId }}</td>
        </tr>
        <tr>
            <th>Inscriptionid</th>
            <td>{{ entity.inscriptionId }}</td>
        </tr>
        <tr>
            <th>Paiementid</th>
            <td>{{ entity.paiementId }}</td>
        </tr>
        <tr>
            <th>Etat</th>
            <td>{{ entity.etat }}</td>
        </tr>
        <tr>
            <th>Montant</th>
            <td>{{ entity.montant }}</td>
        </tr>
        <tr>
            <th>Createdat</th>
            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updatedat</th>
            <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('detailpaiement') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('detailpaiement_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('detailpaiement_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit" data-confirm="êtes vous sûre de vouloir supprimer?">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}