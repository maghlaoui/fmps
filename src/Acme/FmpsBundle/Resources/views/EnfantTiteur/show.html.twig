{% extends '::base.html.twig' %}

{% block body %}
<h3>Détail d'un titeur d'enfant</h3>

<table class="table table-hover">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Enfantid</th>
            <td>{{ entity.enfantId }}</td>
        </tr>
        <tr>
            <th>Titeurid</th>
            <td>{{ entity.titeurId }}</td>
        </tr>
        <tr>
            <th>Créé le</th>
            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Dernière mise à jour</th>
            <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('enfanttiteur') }}">
            <span class="back">Retour &agrave; la liste</span>
        </a>
    </li>
    <li>
        <a href="{{ path('enfanttiteur_edit', { 'id': entity.id }) }}">
            <span class="edit">Editer</span>
        </a>
    </li>
    <li>
        <form action="{{ path('enfanttiteur_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit" data-confirm="êtes vous sûre de vouloir supprimer?">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}