<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Partenaire" table="partenaire">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomPartenaire" type="string" column="nom_partenaire" length="125"/>
    <field name="adressePartenaire" type="text" column="adresse_partenaire"/>
    <field name="tel1Partenaire" type="string" column="tel1_partenaire" length="25"/>
    <field name="tel2Partenaire" type="string" column="tel2_partenaire" length="25"/>
    <field name="faxPartenaire" type="string" column="fax_partenaire" length="25"/>
    <field name="mailPartenaire" type="string" column="mail_partenaire" length="45"/>
    <field name="siteWebPartenaire" type="string" column="site_web_partenaire" length="225"/>
    <field name="villeId" type="integer" column="ville_id"/>
    <field name="createdAt" column="created_at" type="datetime" />
    <field name="updatedAt" column="updated_at" type="datetime" />
  </entity>
</doctrine-mapping>
