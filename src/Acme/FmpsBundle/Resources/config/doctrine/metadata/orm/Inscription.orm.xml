<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Inscription" table="inscription">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numDemande" type="string" column="num_demande" length="255"/>
    <field name="dateDemande" type="date" column="date_demande"/>
    <field name="typeDemande" type="boolean" column="type_demande"/>
    <field name="etatDemande" type="boolean" column="etat_demande"/>
    <field name="dateEntree" type="date" column="date_entree"/>
    <field name="dateSortie" type="date" column="date_sortie"/>
    <field name="adhesionId" type="integer" column="adhesion_id"/>
    <field name="ecoleId" type="integer" column="ecole_id"/>
    <field name="anneeScolaireId" type="integer" column="annee_scolaire_id"/>
    <field name="sectionEcoleId" type="integer" column="section_ecole_id"/>
    <field name="titeurId" type="integer" column="titeur_id"/>
    <field name="enfantId" type="integer" column="enfant_id"/>
    <field name="assurance" type="boolean" column="assurance"/>
    <field name="validated" type="boolean" column="validated"/>
    <field name="createdAt" column="created_at" type="datetime" />
    <field name="updatedAt" column="updated_at" type="datetime" />
  </entity>
</doctrine-mapping>
