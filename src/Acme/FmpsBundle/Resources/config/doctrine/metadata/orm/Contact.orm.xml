<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contact" table="contact">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomContact" type="string" column="nom_contact" length="45"/>
    <field name="prenomContact" type="string" column="prenom_contact" length="45"/>
    <field name="tel1Contact" type="string" column="tel1_contact" length="45"/>
    <field name="tel2Contact" type="string" column="tel2_contact" length="45"/>
    <field name="faxContact" type="string" column="fax_contact" length="45"/>
    <field name="mailContact" type="string" column="mail_contact" length="45"/>
    <field name="statusContact" type="string" column="status_contact" length="245"/>
    <field name="createdAt" column="created_at" type="datetime" />
    <field name="updatedAt" column="updated_at" type="datetime" />
  </entity>
</doctrine-mapping>
