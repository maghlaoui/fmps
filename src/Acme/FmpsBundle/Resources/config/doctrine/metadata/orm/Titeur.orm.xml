<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Titeur" table="titeur">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adherent" type="boolean" column="adherent"/>
    <field name="nom" type="string" column="nom" length="255"/>
    <field name="prenom" type="string" column="prenom" length="255"/>
    <field name="cin" type="string" column="cin" length="255"/>
    <field name="nationalite" type="string" column="nationalite" length="255"/>
    <field name="profession" type="string" column="profession" length="255"/>
    <field name="email" type="string" column="email" length="255"/>
    <field name="telephone" type="string" column="telephone" length="255"/>
    <field name="telephoneBureau" type="string" column="telephone_bureau" length="255"/>
    <field name="fix" type="string" column="fix" length="255"/>
    <field name="adresse" type="string" column="adresse" length="255"/>
    <field name="villeId" type="integer" column="ville_id"/>
    <field name="typeParente" type="boolean" column="type_parente"/>
    <field name="numPpr" type="string" column="num_ppr" length="255" />
    <field name="numAdh" type="string" column="num_adh" length="255"/>
    <field name="createdAt" column="created_at" type="datetime" />
    <field name="updatedAt" column="updated_at" type="datetime" />
  </entity>
</doctrine-mapping>
