<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Employe" table="employe">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="matricule" type="string" column="matricule" length="25"/>
    <field name="nom" type="string" column="nom" length="125"/>
    <field name="prenom" type="string" column="prenom" length="125"/>
    <field name="tel" type="string" column="tel" length="25"/>
    <field name="flote" type="string" column="flote" length="25"/>
    <field name="cin" type="string" column="cin" length="125"/>
    <field name="fichier" type="string" column="fichier" length="255"/>
    <field name="path" type="string" column="path" length="255"/>
    <field name="superieurId" type="integer" column="superieur_id"/>
    <field name="fonctionId" type="integer" column="fonction_id"/>
    <field name="ecoleId" type="integer" column="ecole_id"/>
    <field name="cnss" type="string" column="cnss" length="255"/>
    <field name="rib" type="string" column="rib" length="255"/>
    <field name="adresse" type="string" column="adresse" length="255"/>
    <field name="dateNaissance" type="date" column="date_naissance"/>
    <field name="createdAt" column="created_at" type="datetime" />
    <field name="updatedAt" column="updated_at" type="datetime" />
  </entity>
</doctrine-mapping>
