<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Enseigne" table="enseigne">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id"/>
    <field name="classeId" type="integer" column="classe_id"/>
    <field name="matiereId" type="integer" column="matiere_id"/>
    <field name="sectionEcoleId" type="integer" column="section_ecole_id"/>
    <field name="anneeScolaireId" type="integer" column="annee_scolaire_id"/>
    <field name="dateDebEnseigne" type="date" column="date_deb_enseigne"/>
    <field name="dateFinEnseigne" type="string" column="date_fin_enseigne" length="45"/>
    <field name="createdAt" column="created_at" type="datetime" />
    <field name="updatedAt" column="updated_at" type="datetime" />
  </entity>
</doctrine-mapping>
